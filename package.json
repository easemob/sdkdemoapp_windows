{
  "name": "huanxin",
  "productName": "环信",
  "version": "1.0.1",
  "description": "环信企业即时通讯平台",
  "main": "./app/index.js",
  "scripts": {
    "hot-server": "cross-env NODE_ENV=development node --max_old_space_size=2096 -r babel-register ./devtools/server.js",
    "hot-server:debug": "cross-env NODE_ENV=development node --max_old_space_size=2096 -r babel-register ./devtools/server.js",
    "hot-server:spawn": "npm run hot-server -- --start-hot",
    "build:debug": "cross-env NODE_ENV=development node --max_old_space_size=2096 -r babel-register ./node_modules/webpack/bin/webpack --config ./devtools/webpack.config.development.js --display-reasons",
    "build:app": "cross-env NODE_ENV=production node --max_old_space_size=2096 -r babel-register ./node_modules/webpack/bin/webpack --config ./devtools/webpack.config.development.js --display-reasons",
    "build:main": "cross-env NODE_ENV=production node --max_old_space_size=2096 -r babel-register ./node_modules/webpack/bin/webpack --config ./devtools/webpack.config.electron.js",
    "start": "npm run build:debug && cross-env electron ./app/",
    "start:hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./app/main.development",
    "dist": "electron-builder -m",
    "dist-mac": "electron-builder -m",
    "dist-win": "electron-builder -w --ia32 --x64",
    "production": "npm run build:app && npm run build:main && npm run dist",
    "prod-mac": "npm run build:app && npm run build:main && npm run dist-mac",
    "prod-win": "npm run build:app && npm run build:main && npm run dist-win"
  },
  "build": {
    "extends": "./devtools/electron-builder.json"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "license": "ISC",
  "company": "北京易掌云峰科技有限公司",
